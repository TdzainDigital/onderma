{
  "store.product": {
    "children": [
      "image#home",
      "flex-layout.row#product-breadcrumb",
      "flex-layout.row#product-main",
      "shelf.relatedProducts",
      "flex-layout.row#descricao",
      "rich-text",
      "newsletter"
    ]
  },
  "image#home": {
    "props": {
      "src": "https://onderma.vteximg.com.br/arquivos/TarjaOnderma201810OK.jpg"
    }
  },
  "flex-layout.row#product-breadcrumb": {
    "props": {
      "marginTop": 4
    },
    "children": [
      "breadcrumb"
    ]
  },
  "flex-layout.row#product-main": {
    "props": {
      "colGap": 7,
      "rowGap": 7,
      "marginTop": 4,
      "marginBottom": 7,
      "paddingTop": 7,
      "paddingBottom": 7
    },
    "children": [
      "flex-layout.col#space",
      "flex-layout.col#product-image",
      "flex-layout.col#right-col"
    ]
  },
  "flex-layout.col#product-image": {
    "props": {
      "width": "40%",
      "rowGap": 0
    },
    "children": [
      "product-images"
    ]
  },
  "product-images": {
    "props": {
      "displayThumbnailsArrows": true
    }
  },
  "flex-layout.col#right-col": {
    "props": {
      "width": "30%",
      "preventVerticalStretch": true,
      "rowGap": 0
    },
    "children": [
      "product-name",
      "product-price#product-details",
      "cash-discount",
      "product-separator",
      "flex-layout.row#buy-button",
      "availability-subscriber",
      "shipping-simulator",
      "share#default"
    ]
  },
  "cash-discount": {
    "props": {
      "percentageNumber": 10,
      "minimumPrice": 0
    }
  },
  "rich-text#productDiscount": {
    "props": {
      "text": "10% de desconto a vista",
      "blockClass": "productDiscountText"
    }
  },
  "product-description": {
    "props": {
      "collapseContent": true
    }
  },
  "product-price#product-details": {
    "props": {
      "showInstallments": true,
      "showSavings": true,
      "showListPrice": true,
      "showSellingPriceRange": true,
      "showListPriceRange": true,
      "showLabels": true
    }
  },
  "flex-layout.row#buy-button": {
    "props": {
      "marginTop": 4,
      "marginBottom": 7
    },
    "children": [
      "flex-layout.col#button",
      "flex-layout.col#desc"
    ]
  },
  "flex-layout.col#button": {
    "children": [
      "product-quantity",
      "product-identifier.product",
      "buy-button"
    ]
  },
  "flex-layout.col#desc": {
    "children": [
      "rich-text#duvida",
      "rich-text#duvida2",
      "rich-text#duvidatel"
    ]
  },
  "rich-text#duvida": {
    "props": {
      "text": "**Possui alguma dúvida?**"
    }
  },
  "rich-text#duvida2": {
    "props": {
      "text": "Ligue agora ou utilize nosso atendimento online. Fale com nosso time de especialistas."
    }
  },
  "rich-text#duvidatel": {
    "props": {
      "text": "### (31) 4062-7062",
      "blockClass": "tel-duvidas"
    }
  },
  "share#default": {
    "props": {
      "social": {
        "Facebook": true,
        "WhatsApp": true,
        "Twitter": false,
        "Pinterest": true
      }
    }
  },
  "flex-layout.col#space": {
    "props": {
      "width": "15%"
    },
    "children": []
  },
  "newsletter": {
    "props": {
      "confirmationText": "true",
      "label": "CADASTRE-SE E RECEBA EM SEU E-MAIL, NOVIDADES, PROMOÇÕES E OFERTAS DA LOJA"
    }
  },
  "rich-text": {
    "props": {
      "textPosition": "CENTER",
      "text": "NEWSLETTER",
      "font": "t-heading-5",
      "blockClass": "tt-newsletter"
    }
  },
  "flex-layout.row#descricao": {
    "children": [
      "flex-layout.col#descricao"
    ]
  },
  "flex-layout.col#descricao": {
    "children": [
      "tab-layout#product"
    ]
  },
  "tab-layout#product": {
    "children": [
      "tab-list#product",
      "tab-content#product"
    ],
    "props": {
      "blockClass": "product"
    }
  },
  "tab-list#product": {
    "children": [
      "tab-list.item#tab1prod",
      "tab-list.item#tab2prod"
    ]
  },
  "tab-list.item#tab1prod": {
    "props": {
      "tabId": "tab1prod",
      "label": "DESCRIÇÃO",
      "defaultActiveTab": true
    }
  },
  "tab-list.item#tab2prod": {
    "props": {
      "tabId": "tab2prod",
      "label": "ESPECIFICAÇÕES"
    }
  },
  "tab-content#product": {
    "children": [
      "tab-content.item#tab1prod",
      "tab-content.item#tab2prod"
    ]
  },
  "tab-content.item#tab1prod": {
    "children": [
      "product-description"
    ],
    "props": {
      "tabId": "tab1prod"
    }
  },
  "tab-content.item#tab2prod": {
    "children": [
      "product-specifications"
    ],
    "props": {
      "tabId": "tab2prod"
    }
  }
}